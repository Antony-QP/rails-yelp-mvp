<!DOCTYPE html>
<html>
  <head>
    <title>RailsYelpMvp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-light bg-warning mb-5">
      <span class="navbar-brand mb-0 h1 text-dark">YELP</span>
      <%= link_to 'New', new_restaurant_path, class: 'badge badge-dark' %>
    </nav>
    <%= yield %>
  </body>
</html>
