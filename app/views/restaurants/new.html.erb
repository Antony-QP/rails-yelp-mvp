

<div class="card">
    <div class="card-body text-center">
        <%= simple_form_for @restaurant do |f|%>
        <%= f.input :name %>
        <%= f.input :address %>
        <%= f.input :phone_number %>
        <%= f.input :category %>
        <%= f.submit class: "btn btn-primary"%>
        <% end %>
    </div>
</div>